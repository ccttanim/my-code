#include<bits/stdc++.h>
using namespace std;
int main()
{
    string s1;
    cin>>s1;
    for(int i=0;i<s1.length()-1;++i)
    {
        if(s1[i] != s1[s1.length()-i-1] )
        {
            cout<<"NO";
            break;
        } else
        {
            cout<<"YES";
            break;
        }
    }


}
