#include<bits/stdc++.h>
using namespace std;


int main()
{
    int n,k,min,s=0;
    int t;

    cin>>t;
    while(t--)
    {
        cin>>n>>k;
        int a[n];
        for(int i=0;i<n;i++)
        {
            cin>>a[i];
        }
        min=a[0];
        for(int i=0;i<n;i++)
        {
            if(min>a[i])
            {
                min=a[i];
            }
        }
        if(k-min>0)
        {
            cout<<k-min<<endl;
        }
        else
        {
            cout<<s<<endl;
        }
    }

}
